      ],
      "qwery": "bower_components/qwery/qwery.js",
      "bean": "bower_components/bean/bean.js",
      "reqwest": "bower_components/reqwest/reqwest.js",
      "native-promise-only": "bower_components/native-promise-only/lib/npo.src.js",
